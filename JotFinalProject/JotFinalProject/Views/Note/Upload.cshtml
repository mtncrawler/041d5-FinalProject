
@{
    Layout = "_Layout";
}

<form method="post" enctype="multipart/form-data" asp-controller="Note" asp-action="Post">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload one file using this form:</p>
            <input type="file" name="file" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>